Private Sub Form_Load()

    Dim vTmp As Variant
    Dim sArg As String
    Dim sTmp As String
    
    If IsNull(Me.OpenArgs) Then Exit Sub

    sArg = Me.OpenArgs
    vTmp = Split(sArg, ";")
   
    Select Case vTmp(0)
        Case "SV"
            Me.txtInfoTitre = "La base a été sauvegardée..."
            txtSvBase = vTmp(3): txtSvBase.Visible = True
            txtSvBack = vTmp(2): txtSvBack.Visible = True
            txtSvDoss = vTmp(1): txtSvDoss.Visible = True
        Case Else
            Me.txtInfoTitre = "Résumé des oprérations a faire :"
            sTmp = Replace(sArg, ";", vbCrLf)
            sArg = Replace(sTmp, "!", vbTab)
            Me.txtInfoTxt = sArg
    End Select
End Sub

